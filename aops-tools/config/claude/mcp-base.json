{
  "mcpServers": {
    "zot": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "fastmcp",
        "run",
        "http://services.stoat-musical.ts.net:8024/mcp"
      ]
    },
    "osb": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "fastmcp",
        "run",
        "http://services.stoat-musical.ts.net:8025/mcp"
      ]
    },
    "task_manager": {
      "type": "stdio",
      "command": "uv",
      "args": [
        "run",
        "python",
        "$AOPS/aops-core/mcp_servers/tasks_server.py"
      ],
      "env": {
        "AOPS": "$AOPS",
        "ACA_DATA": "$ACA_DATA"
      }
    }
  }
}