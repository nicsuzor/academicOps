{
  "mcpServers": {
    "gh": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer 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"
      }
    },
    "bmem": { "command": "uvx", "args": ["basic-memory", "mcp"] },
    "zot": {
      "type": "stdio",
      "command": "podman",
      "timeout": 60000,
      "args": [
        "run",
        "--rm",
        "-v",
        "$HOME/.config/gcloud:/root/.config/gcloud:ro",
        "-i",
        "us-central1-docker.pkg.dev/prosocial-443205/reg/zotmcp:latest"
      ]
    },
    "osb": {
      "type": "stdio",
      "command": "podman",
      "timeout": 60000,
      "args": [
        "run",
        "-v",
        "$HOME/.config/gcloud:/root/.config/gcloud:ro",
        "-i",
        "us-central1-docker.pkg.dev/prosocial-443205/reg/osbchatmcp:latest"
      ]
    },
    "gemini": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "gemini-mcp-tool"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "outlook": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/nicsuzor/omcp",
        "outlook-mcp"
      ]
    },
    "task_manager": {
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "cd $AOPS && PYTHONPATH=$AOPS python3 skills/tasks/server.py"
      ]
    }
  }
}
