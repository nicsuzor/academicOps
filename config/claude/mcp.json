{
  "mcpServers": {
    "gh": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer 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"
      }
    },
    "memory": {
      "type": "stdio",
      "command": "uv",
      "args":["--directory", "/home/debian/mcp-memory-service", "run", "memory"],
      "env": {
        "MCP_MEMORY_STORAGE_BACKEND": "sqlite_vec",
        "MCP_MEMORY_SQLITE_PRAGMAS": "busy_timeout=15000,cache_size=20000",
        "LOG_LEVEL": "INFO"
      }
    },
    "zot": {
      "type": "stdio",
      "command": "podman",
      "timeout": 60000,
      "args": [
        "run",
        "--rm",
        "-v",
        "$HOME/.config/gcloud:/root/.config/gcloud:ro",
        "-i",
        "us-central1-docker.pkg.dev/prosocial-443205/reg/zotmcp:latest"
      ]
    },
    "osb": {
      "type": "stdio",
      "command": "podman",
      "timeout": 60000,
      "args": [
        "run",
        "-v",
        "$HOME/.config/gcloud:/root/.config/gcloud:ro",
        "-i",
        "us-central1-docker.pkg.dev/prosocial-443205/reg/osbchatmcp:latest"
      ]
    },
    "gemini": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "gemini-mcp-tool"
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "outlook": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "fastmcp",
        "run",
        "http://nicwin.stoat-musical.ts.net:8023/mcp"
      ],
      "env": {}
    },
    "task_manager": {
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "cd $AOPS && PYTHONPATH=$AOPS python3 skills/tasks/server.py"
      ]
    }
  }
}
