{
  "general": {
    "checkpointing": {
      "enabled": true
    }
  },
  "security": {
    "folderTrust": {
      "featureEnabled": true,
      "enabled": true
    },
    "auth": {
      "selectedType": "vertex-ai"
    }
  },
  "ui": {
    "hideWindowTitle": true,
    "showMemoryUsage": true
  },
  "ide": {
    "enabled": true
  },
  "mcpServers": {
    "gh": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "zot": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-v",
        "/home/nic/.config/gcloud:/root/.config/gcloud:ro",
        "-i",
        "us-central1-docker.pkg.dev/prosocial-443205/reg/zotmcp:latest"
      ]
    },
    "omcpproxy": {
      "command": "uv",
      "args": [
        "run",
        "--with",
        "fastmcp",
        "fastmcp",
        "run",
        "/home/nic/src/omcp/outlook_mcp/proxy.py"
      ]
    }
  },
  "darkMode": "light",
  "scale": 0,
  "locale": "en-US"
}