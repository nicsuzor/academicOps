{
  "claude": {
    "mcpServers": {
      "memory": {
        "type": "http",
        "url": "http://services.stoat-musical.ts.net:8026/mcp",
        "headers": {
          "Authorization": "Bearer $MCP_MEMORY_API_KEY"
        }
      },
      "task_manager": {
        "type": "stdio",
        "command": "uv",
        "args": [
          "run",
          "--directory",
          "${CLAUDE_PLUGIN_ROOT}",
          "python",
          "mcp_servers/tasks_server.py"
        ],
        "env": {
          "ACA_DATA": "${ACA_DATA}"
        }
      }
    }
  },
  "gemini": {
    "mcpServers": {
      "memory": {
        "url": "http://services.stoat-musical.ts.net:8026/mcp",
        "headers": {
          "Authorization": "Bearer $MCP_MEMORY_API_KEY"
        }
      },
      "task_manager": {
        "command": "uv",
        "args": [
          "run",
          "--directory",
          "${extensionPath}",
          "python",
          "mcp_servers${/}tasks_server.py"
        ],
        "env": {
          "ACA_DATA": "${ACA_DATA}"
        }
      }
    }
  }
}